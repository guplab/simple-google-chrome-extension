{
    "manifest_version": 2,
    "name": "Guplab Launcher",
    "description": "Quick Launch Guplab",
    "version": "0.0.1",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}